{
    "name": "back-end",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "npx @nestjs/cli build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "npx @nestjs/cli start",
        "start:dev": "npx @nestjs/cli start --watch",
        "start:debug": "npx @nestjs/cli start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node ./node_modules/typeorm/cli",
        "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/configs/migration/typeorm.config-migrations.ts",
        "typeorm:generate-migration": "npm run typeorm -- -d ./src/configs/migration/typeorm.config-migrations.ts migration:generate ./src/database/migrations/$npm_config_name",
        "typeorm:create-migration": "npm run typeorm -- migration:create ./src/database/migrations/$npm_config_name",
        "typeorm:revert-migration": "npm run typeorm -- -d ./src/config/typeorm/typeorm.config.ts migration:revert",
        "seed:config": "typeorm-seeding config -r ./dist -c configs/seeder.config.js",
        "seed:run": "typeorm-seeding seed -r ./dist -c configs/seeder.config.js",
        "db:refresh": "npm run typeorm:cli schema:drop && npm run migration:run && npm run seed:run"
    },
    "dependencies": {
        "@concepta/typeorm-seeding": "^4.0.0-beta.0",
        "@nestjs/axios": "^0.1.0",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/jwt": "^9.0.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/platform-socket.io": "^9.1.6",
        "@nestjs/typeorm": "^9.0.0",
        "@nestjs/websockets": "^9.1.4",
        "@ngneat/falso": "^6.1.0",
        "@types/bcrypt": "^5.0.0",
        "@types/express-session": "^1.17.5",
        "@types/multer": "^1.4.7",
        "argon2": "^0.29.1",
        "axios": "^0.27.2",
        "bcrypt": "^5.1.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "crypto": "^1.0.1",
        "dotenv": "^16.0.2",
        "express-session": "^1.17.3",
        "express-sessions": "^1.0.6",
        "fs": "^0.0.1-security",
        "multer": "^1.4.5-lts.1",
        "nestjs": "0.0.1",
        "otplib": "^12.0.1",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "passport-oauth2": "^1.6.1",
        "pg": "^8.7.3",
        "qrcode": "^1.5.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "socket.io": "^4.5.2",
        "supports-color": "^9.3.1",
        "typeorm": "^0.3.7"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@nestjs/common": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@types/babel__core": "^7.1.19",
        "@types/express": "^4.17.13",
        "@types/jest": "28.1.4",
        "@types/node": "^16.11.47",
        "@types/passport-local": "^1.0.34",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "express": "^4.18.1",
        "jest": "28.1.2",
        "joi": "^17.6.0",
        "passport-42": "^1.2.6",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "28.0.5",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "4.0.0",
        "typescript": "^4.3.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
